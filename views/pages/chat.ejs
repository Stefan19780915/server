<%- include('../partials/head') %>
<body>
  <%- include('../partials/msg') %> <%- include('../partials/header') %>

  <section class="secondary-section">
    <div class="container">
      <div class="chat-messages flow"><span></span></div>
    </div>
  </section>

  <section class="primary-section absolute">
    <div class="container">
      <form id="chat-form">
        <input
          type="text"
          id="msg"
          placeholder="Enter Message..."
          required
          autocomplete="off"
        />
        <button id="chat-submit" class="btn-primary">Send</button>
      </form>
    </div>
  </section>

  <script src="/socket.io/socket.io.js"></script>
  <script src="js/main.js"></script>
</body>
<%- include('../partials/footer') %>
